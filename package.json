{
  "name": "project__electron__tutorials",
  "version": "0.1.0",
  "private": true,
  "description": "Electron Boilder Plate App",
  "author": {
    "name": "KangSeohyeon"
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.3.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.11.41",
    "@types/react": "^18.0.12",
    "@types/react-dom": "^18.0.5",
    "@types/react-router-dom": "^5.3.3",
    "@types/styled-components": "^5.1.25",
    "electron-builder-squirrel-windows": "^23.3.1",
    "electron-is-dev": "^2.0.0",
    "fs": "0.0.1-security",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.3.0",
    "react-scripts": "5.0.1",
    "recoil": "^0.7.3",
    "styled-components": "^5.3.5",
    "typescript": "^4.7.3",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "electron": "^19.0.4",
    "electron-builder": "^23.1.0"
  },
  "build": {
    "appId": "planet-node-electron",
    "files": [
      "build/**/*",
      "node_modules/**/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64",
            "ia32"
          ]
        }
      ],
      "icon": "./build/logo512.png"
    },
    "squirrelWindows": {
      "iconUrl": "https://www.gitbook.com/cdn-cgi/image/height=40,fit=contain,dpr=2,format=auto/https%3A%2F%2F170524761-files.gitbook.io%2F~%2Ffiles%2Fv0%2Fb%2Fgitbook-x-prod.appspot.com%2Fo%2Fspaces%252F-McMmuxKCCgDfbGouV8t%252Flogo%252FoybG2CTmnrj1djHTcRwc%252FICON%2520DEV%2520PORTAL.png%3Falt%3Dmedia%26token%3D84f92e2d-d680-4e4c-84ca-7962577c34ee"
    }
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "react-start": "BROWSER=none npm start",
    "electron-start": "electron .",
    "dist": "electron-builder --mwl --c.extraMetadata.main=build/electron.js",
    "dist:mac": "electron-builder --mac --c.extraMetadata.main=build/electron.js",
    "dist:windows": "electron-builder --win --ia32 --c.extraMetadata.main=build/electron.js"
  },
  "main": "public/electron.js",
  "homepage": "./",
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
